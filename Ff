local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/jensonhirst/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Ez Hub V2", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local Tab = Window:MakeTab({
	Name = "Forsaken",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddButton({
	Name = "Auto block!",
	Callback = function()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/RonitScripts/Befr/refs/heads/main/Imfr"))()
      		print("button pressed")
  	end    
})

